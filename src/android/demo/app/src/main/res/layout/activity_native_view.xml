<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".NativeActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/native_crash_java_kotlin_data_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Crash Java/Kotlin" />

        <Button
            android:id="@+id/native_crash_c_cpp_data_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Crash C/C++" />

        <Button
            android:id="@+id/native_long_task_data_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="UIBlock(Longtask)" />

        <Button
            android:id="@+id/native_dynamic_rum_tag_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tag 动态设置" />


    </LinearLayout>
</androidx.core.widget.NestedScrollView>